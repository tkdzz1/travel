<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
				 "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.himedia.springboot.foodstoreDAO">

<select id="get_fs" resultType="com.himedia.springboot.foodstoreDTO">
   select * from foodstore;
</select>

<select id="cntFoodList" resultType="int">
	select count(*) from foodstore;
</select>

<select id="getList" resultType="com.himedia.springboot.foodstoreDTO">
	select * from foodstore limit #{param1}, #{param2}
</select>

</mapper>